{
  "name": "simvolio-support",
  "displayName": "Simvolio support",
  "description": "Simvolio and Protypo support",
  "version": "0.0.3",
  "publisher": "hy6",
  "author": {
    "name": "Andrey Voronkov",
    "email": "cynephy6@gmail.com"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Languages"
  ],
  "keywords": [
    "protypo",
    "simvolio",
    "apla",
    "beautify"
  ],
  "contributes": {
    "languages": [{
      "id": "simvolio",
      "aliases": [
        "simvolio",
        "protypo"
      ],
      "extensions": [
        ".sim",
        ".pt"
      ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "simvolio",
      "scopeName": "source.simvolio",
      "path": "./syntaxes/simvolio.tmLanguage.json"
    }]
  },
  "activationEvents": [
    "onLanguage:simvolio",
    "onLanguage:protypo"
  ],
  "main": "src/extension",
  "devDependencies": {
    "vscode": "^1.1.8"
  },
  "dependencies": {
    "vscode-languageclient": "^3.5.0",
    "vscode-languageserver": "^3.5.0"
  }
}