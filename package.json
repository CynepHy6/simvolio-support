{
  "name": "simvolio-support",
  "displayName": "Simvolio support",
  "description": "Simvolio and Protypo support",
  "version": "0.3.2",
  "publisher": "hy6",
  "author": {
    "name": "Andrey Voronkov",
    "email": "cynephy6@gmail.com"
  },
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Languages"
  ],
  "keywords": [
    "protypo",
    "simvolio",
    "apla"
  ],
  "contributes": {
    "languages": [{
        "id": "simvolio",
        "extensions": [
          ".sim"
        ],
        "configuration": "./language-configuration_simvolio.json"
      },
      {
        "id": "protypo",
        "extensions": [
          ".ptl"
        ],
        "configuration": "./language-configuration_protypo.json"
      }
    ],
    "grammars": [{
        "language": "simvolio",
        "scopeName": "source.simvolio",
        "path": "./syntaxes/simvolio.tmLanguage.json"
      },
      {
        "language": "protypo",
        "scopeName": "source.protypo",
        "path": "./syntaxes/protypo.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:simvolio",
    "onLanguage:protypo"
  ],
  "main": "src/extension",
  "devDependencies": {
    "vscode": "^1.1.8"
  },
  "dependencies": {
    "vscode": "^1.1.8"
  }
}